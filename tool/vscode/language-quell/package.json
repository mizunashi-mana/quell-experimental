{
    "name": "language-quell",
    "displayName": "Quell Language",
    "description": "Syntax highlighting for Quell",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.46.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "quell",
                "aliases": ["Quell", "quell"],
                "extensions": [".qq"],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "quell",
                "scopeName": "source.quell",
                "path": "./syntaxes/quell.tmLanguage.json"
            }
        ]
    }
}
